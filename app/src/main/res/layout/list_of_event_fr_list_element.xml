<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="@dimen/list_of_event_fr_list_element_height"
    android:divider="@null"
    android:dividerPadding="0dp"
    android:paddingTop="10dp"
    android:paddingBottom="10dp">

    <android.support.v7.widget.CardView
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:id="@+id/card_view_outer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        card_view:cardBackgroundColor="@android:color/transparent"
        card_view:cardCornerRadius="0dp"
        card_view:cardElevation="3dp" >

        <android.support.v7.widget.CardView
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:id="@+id/card_view_inner"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_marginTop="3dp"
            card_view:cardBackgroundColor="@color/white"
            card_view:cardCornerRadius="4dp"
            card_view:cardElevation="0dp" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="1dp"
                android:layout_marginBottom="3dp"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:orientation="vertical"
                android:background="@android:color/transparent"
                android:gravity="center_vertical"
                android:paddingLeft="22dp"
                android:paddingRight="22dp"
                >

                <iabc.com.voli.iabcandroid.ui.custom.CTextView
                    android:id="@+id/list_of_event_fr_list_element_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:ellipsize="end"
                    android:text="@string/app_def_text_short"
                    app:plusTextSize="@integer/plus_text_size_for_16"
                    app:font_name="AVERNIR_NEXT"
                    />
                <iabc.com.voli.iabcandroid.ui.custom.CTextView
                    android:id="@+id/list_of_event_fr_list_element_category_location"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/_A1A1A1"
                    app:plusTextSize="@integer/plus_text_size_for_11"
                    app:font_name="AVERNIR_NEXT"
                    android:text="@string/app_def_text_short"
                    />


            </LinearLayout>

        </android.support.v7.widget.CardView>
    </android.support.v7.widget.CardView>

